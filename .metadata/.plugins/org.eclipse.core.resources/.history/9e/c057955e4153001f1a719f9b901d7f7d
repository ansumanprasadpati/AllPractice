import java.util.Arrays;

public class RemoveDuplicate {

	public static int main(String[] args) {
		// TODO Auto-generated method stub
		int[] arr= {1,1,1,4,6,7,8,3,2};
		Arrays.sort(arr);
		int j=0;
		for(int i=0;i<arr.length;i++)
		{
			if(arr[i]!=arr[j])
			{
				j++;
				arr[j]=arr[i];
			}
		} return j+1;
	}

}
